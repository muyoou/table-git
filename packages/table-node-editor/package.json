{
  "name": "@table-git/node-editor",
  "version": "0.1.0",
  "description": "PixiJS-powered visual node editor for table-memory-engine workflows",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "test": "jest",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "node-editor",
    "workflow",
    "pixi",
    "table-git"
  ],
  "author": "Table Version Control Team",
  "license": "MIT",
  "dependencies": {
    "@table-git/memory-engine": "file:../table-memory-engine",
    "pixi.js": "^7.4.2"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "jest": "^29.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  }
}
